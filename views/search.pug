extends layouts/main
include mixins/videoBlock

block content 
    .search__header
        h3 Searching for: #{searchingBy}
    .search__vieos 
        if videos.length === 0
            h5 No Videos Found
        each videoItem in videos
            +videoBlock({
                title: videoItem.title,
                views: videoItem.views,
                videoFile: videoItem.fileUrl,
                id: videoItem.id
            })

